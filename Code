#include <iostream>
#include<algorithm>
using namespace std;

int main() {
       int T,a[3];std::cin >> T;
       while(T--)
       {
           for(int i=0;i<3;i++)
           {
               std::cin >> a[i];
           }
           sort(a,a+3);
           std::cout << a[1] << std::endl;
           
       }
    
	
	return 0;
}
